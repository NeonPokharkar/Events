<?xml version="1.0" encoding="UTF-8"?>
<web-app
        xmlns="http://xmlns.jcp.org/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
        version="4.0">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>HomePage</servlet-name>
    <servlet-class>servlet.HomePageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HomePage</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginPage</servlet-name>
    <servlet-class>servlet.LoginPageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginPage</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SignUpPage</servlet-name>
    <servlet-class>servlet.SignUpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SignUpPage</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DashBoard</servlet-name>
    <servlet-class>servlet.DashboardServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DashBoard</servlet-name>
    <url-pattern>/dashboard</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Booking</servlet-name>
    <servlet-class>servlet.BookingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Booking</servlet-name>
    <url-pattern>/booking</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdminConsole</servlet-name>
    <servlet-class>servlet.AdminConsoleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminConsole</servlet-name>
    <url-pattern>/adminconsole</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CreateEvent</servlet-name>
    <servlet-class>servlet.CreateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreateEvent</servlet-name>
    <url-pattern>/createEvent</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CreateVenue</servlet-name>
    <servlet-class>servlet.CreateVenueServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreateVenue</servlet-name>
    <url-pattern>/createVenue</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DeleteEvent</servlet-name>
    <servlet-class>servlet.DeleteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteEvent</servlet-name>
    <url-pattern>/deleteEvent</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DeleteVenue</servlet-name>
    <servlet-class>servlet.DeleteVenueServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteVenue</servlet-name>
    <url-pattern>/deleteVenue</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DeleteBooking</servlet-name>
    <servlet-class>servlet.DeleteBookingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteBooking</servlet-name>
    <url-pattern>/deleteBooking</url-pattern>
  </servlet-mapping>
</web-app>
